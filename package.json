{
  "name": "markster",
  "version": "0.0.2",
  "description": "a web component for highlighting text",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/markster/markster.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "1.17.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "mark",
    "marker",
    "highlight",
    "highlight text",
    "highlighter",
    "stencil",
    "stenciljs",
    "web component",
    "typescript",
    "component",
    "custom element"
  ],
  "homepage": "https://github.com/adamlacombe/markster",
  "author": {
    "name": "Adam LaCombe",
    "web": "https://github.com/adamlacombe"
  },
  "contributors": [
    {
      "name": "Adam LaCombe",
      "web": "https://github.com/adamlacombe"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adamlacombe/markster.git"
  },
  "bugs": {
    "url": "https://github.com/adamlacombe/markster/issues"
  },
  "license": "MIT"
}